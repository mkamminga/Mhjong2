{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "build": "tsc -p src/ --outDir dist/",
    "build:watch": "tsc -p src/ -w --outDir dist/",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "sass:build": "node-sass src/styles/main.scss -o dist/styles/",
    "sass:watch": "npm run sass:build & node-sass src/styles/main.scss -wo dist/styles/",
    "cpy:watch": "cpx \"src/**/*.{html,png,jpg,js}\" dist --watch",
    "start": "concurrently \"npm run build:watch\" \"npm run cpy:watch\" \"npm run sass:watch\" \"npm run serve\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"npm run cpy:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.7",
    "@angular/compiler-cli": "2.4.5",
    "@angular/core": "~2.4.7",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.7",
    "@angular/platform-browser-dynamic": "~2.4.7",
    "@angular/tsc-wrapped": "^0.5.0",
    "@angular/upgrade": "2.4.5",
    "@angular/router": "~3.4.0",
    "@types/socket.io-client": "^1.4.29",
    "angular-in-memory-web-api": "~0.2.4",
    "core-js": "^2.4.1",
    "cpx": "^1.5.0",
    "foundation-sites": "^6.3.0",
    "rxjs": "5.0.1",
    "socket.io": "1.2.0",
    "socket.io-adapter": "^1.1.0",
    "socket.io-client": "^1.7.3",
    "systemjs": "0.19.40",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    
    "concurrently": "^3.2.0",
